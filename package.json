{
  "name": "egg-szjcomo",
  "version": "1.0.0",
  "description": "基于egg.js的快速开发框架",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "await-stream-ready": "^1.0.1",
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-redis": "^2.4.0",
    "egg-router-plus": "^1.3.1",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^5.2.2",
    "egg-szjcomo-cache": "^1.0.1",
    "egg-szjcomo-dyw": "^1.0.2",
    "egg-szjcomo-excel": "^1.0.4",
    "egg-szjcomo-ffmpeg": "^1.0.1",
    "egg-szjcomo-utils": "^1.0.1",
    "egg-szjcomo-validate": "^1.0.0",
    "egg-szjcomo-wxpay": "^1.0.0",
    "mysql2": "^2.1.0",
    "node-qiniu-sdk": "^1.6.6",
    "stream-wormhole": "^1.1.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-szjcomo --ignore-stderr --workers=1",
    "stop": "egg-scripts stop --title=egg-szjcomo --workers=1",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "model": "egg-sequelize-auto -o app/models/mysql -c auto_model.json -t szj_wxpay"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "szjcomo",
  "license": "MIT"
}
